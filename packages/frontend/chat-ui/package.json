{
  "name": "@mozaiks/chat-ui",
  "version": "0.1.0",
  "description": "Chat UI components for Mozaiks platform",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.js --format esm,cjs --out-dir dist --external react --external react-dom --external react-router-dom"
  },
  "dependencies": {
    "dompurify": "^3.2.6",
    "marked": "^16.1.2"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-router-dom": ">=6.0.0"
  },
  "devDependencies": {
    "tsup": "^8.3.6"
  },
  "keywords": [
    "mozaiks",
    "chat",
    "ui",
    "react"
  ],
  "license": "MIT"
}
