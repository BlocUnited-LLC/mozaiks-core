{
  "MongoDB": {
    "ConnectionString": "mongodb://microserviceMongoDB:27017",
    "DatabaseName": "MozaiksDB"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.Extensions.DependencyInjection": "Debug"
    }
  },
  "ApiKeys": {
    "Environment": "test",
    "KeyLength": 32
  },
  "GitHub": {
    "AccessToken": "replace_me",
    "OrganizationName": "",
    "MozaiksCoreRepoUrl": "https://github.com/BlocUnited-LLC/mozaiks-core",
    "MozaiksCoreBranch": "main",
    "MozaiksCoreSubdirectory": "sdk"
  },
  "GitHubSecrets": {
    "DockerHubUsername": "",
    "DockerHubToken": "",
    "AdditionalSecrets": {}
  },
  "Deployment": {
    "PlatformApiUrl": "http://localhost:8010",
    "BundleTempPath": "",
    "MaxBundleSizeBytes": 104857600
  },
  "DatabaseProvisioning": {
    "AdminConnectionString": "",
    "MetadataDatabase": "mozaiks_platform"
  },
  "MonetizationPolicy": {
    "PlanPriceFloorCents": 0,
    "PlanPriceCeilingCents": 1000000,
    "AddOnPriceFloorCents": 0,
    "AddOnPriceCeilingCents": 1000000,
    "TransactionFeeBpsFloor": 0,
    "TransactionFeeBpsCeiling": 3000,
    "MaxPriceDeltaBps": 5000,
    "MaxTransactionFeeDeltaBps": 500,
    "CooldownMinutes": 60,
    "TokensMonthlyFloor": 0,
    "TokensMonthlyCeiling": 1000000,
    "DomainsMaxFloor": 0,
    "DomainsMaxCeiling": 1000,
    "EmailMaxDomainsFloor": 0,
    "EmailMaxDomainsCeiling": 1000
  },
  "AllowedHosts": "*",
  "AllowedOrigins": "http://localhost:3000, https://moz-dev.vercel.app",
  "MAIL_SERVER_API_KEY": "",
  "HostingApi": {
    "BaseUrl": "http://localhost:8050",
    "InternalApiKey": ""
  },
  "Payments": {
    "SecretKey": "",
    "WebhookSecret": ""
  },
  "AzureBlobStorage": {
    "ConnectionString": "YourAzureBlobStorageConnectionString",
    "ContainerName": "your-container-name"
  },
  "MicroServiceEndpoints": {
    "NotificationService": "https://moz-notifications.azurewebsites.net/api",
    "GovernanceService": "http://localhost:8005"
  },
  "Redis": {
    "ConnectionString": ""
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.MongoDB" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "MongoDB",
        "Args": {
          "databaseUrl": "mongodb://microserviceMongoDB:27017/MozaiksDB",
          "collectionName": "ServerLogs"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "AuthServer"
    }
  }

}
